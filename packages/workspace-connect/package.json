{
  "name": "workspace-connect",
  "version": "0.0.3",
  "description": "Gmail integration SDK for Next.js with extensible provider architecture",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.mjs",
      "require": "./dist/next/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src"
  },
  "keywords": [
    "gmail",
    "outlook",
    "email",
    "workspace",
    "oauth",
    "nextjs",
    "pubsub",
    "webhooks"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/inboundemail/workspace-connect"
  },
  "homepage": "https://github.com/inboundemail/workspace-connect#readme",
  "bugs": {
    "url": "https://github.com/inboundemail/workspace-connect/issues"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsup": "^8.0.0",
    "typescript": "^5"
  },
  "dependencies": {
    "googleapis": "^166.0.0",
    "@google-cloud/pubsub": "^5.2.0"
  }
}

